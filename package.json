{
	"name": "aimhigher",
	"version": "6.0.0",
	"description": "Website for AimHigher Web Design",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AimHigher-Web-Design/aimhigher.git"
	},
	"scripts": {
		"start": "npm run blog && npm run develop -- --port $PORT",
		"develop": "gatsby develop",
		"build": "npm run blog && echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && gatsby build",
		"build:fab": "npm run build && npm run fab:compile",
		"fab:compile": "fab-static public",
		"test:a11y": "pa11y --reporter cli http://localhost:8000 > a11yreport.txt",
		"clean": "rm -rf .cache/ public/",
		"blog:posts": "git submodule init && git submodule update --remote",
		"blog": "npm run blog:posts && cp -r src/blog/img/* static/img/blog"
	},
	"pre-commit": [],
	"author": "AimHigher Web Design",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/AimHigher-Web-Design/aimhigher/issues"
	},
	"homepage": "https://github.com/AimHigher-Web-Design/aimhigher#readme",
	"dependencies": {
		"@raae/gatsby-remark-oembed": "^0.1.1",
		"babel-eslint": "^10.0.1",
		"babel-plugin-styled-components": "^1.10.1",
		"eslint": "^6.6.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.1.1",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-standard": "^4.0.0",
		"gatsby": "^2.17.11",
		"gatsby-image": "^2.2.31",
		"gatsby-plugin-google-tagmanager": "^2.0.13",
		"gatsby-plugin-netlify": "^2.1.24",
		"gatsby-plugin-netlify-cms": "^4.1.27",
		"gatsby-plugin-react-helmet": "^3.0.12",
		"gatsby-plugin-react-svg": "^2.1.1",
		"gatsby-plugin-sharp": "^2.2.37",
		"gatsby-plugin-styled-components": "^3.0.7",
		"gatsby-remark-copy-linked-files": "^2.0.12",
		"gatsby-remark-figure-caption": "^2.0.0",
		"gatsby-remark-images": "^3.0.13",
		"gatsby-remark-prettier": "^1.0.0",
		"gatsby-remark-relative-images": "^0.2.2",
		"gatsby-source-filesystem": "^2.0.36",
		"gatsby-transformer-remark": "^2.3.12",
		"gatsby-transformer-sharp": "^2.3.3",
		"netlify-cms-app": "^2.9.7",
		"prettier": "^1.17.1",
		"react": "^16.8.6",
		"react-async-script": "^1.0.1",
		"react-dom": "^16.8.6",
		"react-feather": "^2.0.3",
		"react-google-recaptcha": "^2.0.1",
		"react-helmet": "^5.2.1",
		"react-media": "^1.9.2",
		"react-svg": "^10.0.2",
		"react-twitter-widgets": "^1.7.1",
		"recompose": "^0.30.0",
		"styled-components": "^4.3.1"
	},
	"devDependencies": {
		"@fab/static": "^0.1.4",
		"jest": "^24.8.0",
		"pre-commit": "^1.2.2"
	}
}