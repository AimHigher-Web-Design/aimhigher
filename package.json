{
	"name": "aimhigher",
	"version": "6.0.0",
	"description": "Website for AimHigher Web Design",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AimHigher-Web-Design/aimhigher.git"
	},
	"scripts": {
		"start": "npm run blogs && npm run develop",
		"develop": "gatsby develop --https",
		"build": "npm run blogs && gatsby build",
		"serve": "gatsby serve",
		"tests": "npm run a11y",
		"a11y": "pa11y --reporter cli http://localhost:8000 > a11yreport.txt",
		"clean": "rm -rf .cache/ public/",
		"blogImages": "cp src/blog/img/* static/img/blog/",
		"updatePosts": "git submodule update --remote",
		"blogs": "npm run updatePosts && npm run blogImages"
	},
	"pre-commit": [],
	"author": "AimHigher Web Design",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/AimHigher-Web-Design/aimhigher/issues"
	},
	"homepage": "https://github.com/AimHigher-Web-Design/aimhigher#readme",
	"dependencies": {
		"axios": "^0.18.0",
		"bluebird": "^3.5.2",
		"emotion": "^9.2.12",
		"emotion-server": "^9.2.12",
		"emotion-theming": "^9.1.2",
		"gatsby": "2.0.17",
		"gatsby-image": "^2.0.12",
		"gatsby-plugin-emotion": "^2.0.5",
		"gatsby-plugin-google-tagmanager": "^2.0.5",
		"gatsby-plugin-netlify": "^2.0.1",
		"gatsby-plugin-netlify-cms": "next",
		"gatsby-plugin-react-helmet": "^3.0.0-rc.1",
		"gatsby-plugin-react-svg": "^2.0.0",
		"gatsby-plugin-sharp": "^2.0.0-rc.7",
		"gatsby-remark-images": "^2.0.3",
		"gatsby-source-filesystem": "^1.5.39",
		"gatsby-transformer-remark": "^1.7.44",
		"gatsby-transformer-sharp": "^2.1.1",
		"netlify-cms": "^2.1.1",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-emotion": "^9.2.12",
		"react-feather": "^1.1.4",
		"react-helmet": "^5.2.0",
		"react-icons": "^2.2.7",
		"react-media": "^1.6.1",
		"react-svg": "^6.0.17",
		"react-twitter-widgets": "^1.7.1"
	},
	"devDependencies": {
		"jest": "^23.6.0",
		"pre-commit": "^1.2.2"
	}
}
